[req]
prompt = no
encrypt_key = no
default_md = sha256
default_bits = 4096
distinguished_name = dn
x509_extensions = v3_req

[dn]
C = TW
ST = Taiwan
L = Taichung
O = LIS
OU = RD
CN = elasticsearch

[v3_req]
subjectAltName = @alt_names

[alt_names]
DNS.1 = localhost
DNS.2 = opensearch-node1
DNS.3 = opensearch-node2
IP.1 = 127.0.0.1
